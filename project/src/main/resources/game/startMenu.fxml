<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox fx:id="menuContainer" alignment="TOP_CENTER" minWidth="1200.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="startButtonID" mnemonicParsing="false" prefWidth="10000.0" stylesheets="@../styles/button.css" text="START" textFill="WHITE" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="DejaVu Sans Bold" size="23.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" prefWidth="10000.0" stylesheets="@../styles/button.css" text="LEVEL" textFill="WHITE" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="DejaVu Sans Bold" size="23.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" prefWidth="10000.0" stylesheets="@../styles/button.css" text="SCORE" textFill="WHITE" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="DejaVu Sans Bold" size="23.0" />
         </font>
      </Button>
   </children>
</HBox>
